  {
   "cell_type": "code",
   "execution_count": 302,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Score ajdusted\n",
    "real_rate_upd = len(df_bin[df_bin.target==1]) / len(df_bin) #пересчет real_rate\n",
    "df_bin['Adj_P1_upd'] = (1/(1 + ((1/real_rate_upd) - 1) / ((1/0.5) - 1) * ((1/df_bin.y_score) - 1)))"
   ]
  },
